+		};
+
+		pins_mdio {
+			groups = "tsn0_mdio";
+			drive-strength = <3>;
+			power-source = <3300>;
+		};
+	};
+
+	tsn1_pins: tsn1 {
+		mux {
+			groups = "tsn1_link", "tsn1_mdio";
+			function = "tsn1";
+			drive-strength = <3>;
+			power-source = <3300>;
+		};
+
+		pins_mdio {
+			groups = "tsn1_mdio";
+			drive-strength = <3>;
+			power-source = <3300>;
+		};
+	};
+
+	tsn2_pins: tsn2 {
+		mux {
+			groups = "tsn2_link", "tsn2_mdio";
+			function = "tsn2";
+			drive-strength = <3>;
+			power-source = <3300>;
+		};
+
+		pins_mdio {
+			groups = "tsn2_mdio";
+			drive-strength = <3>;
