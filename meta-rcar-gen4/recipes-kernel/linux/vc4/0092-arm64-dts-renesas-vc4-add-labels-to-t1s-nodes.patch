From 1c72eff48f898b00c0a81c8630898b8d19b83c65 Mon Sep 17 00:00:00 2001
From: Nikita Yushchenko <nikita.yoush@cogentembedded.com>
Date: Mon, 14 Aug 2023 09:20:34 +0600
Subject: [PATCH 92/92] arm64: dts: renesas: vc4: add labels to t1s nodes

Labels are useful for cleaner overlay files.

Signed-off-by: Nikita Yushchenko <nikita.yoush@cogentembedded.com>
---
 arch/arm64/boot/dts/renesas/r8a779f0-vc4-ctrl-domain.dts | 6 +++---
 1 file changed, 3 insertions(+), 3 deletions(-)

diff --git a/arch/arm64/boot/dts/renesas/r8a779f0-vc4-ctrl-domain.dts b/arch/arm64/boot/dts/renesas/r8a779f0-vc4-ctrl-domain.dts
index 95ebcee3a6bb..06dd7828716d 100644
--- a/arch/arm64/boot/dts/renesas/r8a779f0-vc4-ctrl-domain.dts
+++ b/arch/arm64/boot/dts/renesas/r8a779f0-vc4-ctrl-domain.dts
@@ -212,7 +212,7 @@ &mspi0 {
 	pinctrl-0 = <&mspi0_pins>;
 	pinctrl-names = "default";
 
-	t1s@0 {
+	t1s0: t1s@0 {
 		compatible = "microchip,lan865x";
 		reg = <0>;
 
@@ -238,7 +238,7 @@ &mspi1 {
 	pinctrl-0 = <&mspi1_pins>;
 	pinctrl-names = "default";
 
-	t1s@0 {
+	t1s1: t1s@0 {
 		compatible = "microchip,lan865x";
 		reg = <0>;
 
@@ -264,7 +264,7 @@ &mspi4 {
 	pinctrl-0 = <&mspi4_pins>;
 	pinctrl-names = "default";
 
-	t1s@0 {
+	t1s2: t1s@0 {
 		compatible = "microchip,lan865x";
 		reg = <0>;
 
-- 
